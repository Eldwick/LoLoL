<script type="text/javascript">
  var lists = <%= @lists %>
</script>
<div ng-app=myApp>
  <div ng-controller="GreetingController">
    <h1>{{ title }}</h1>
  </div>
  <input placeholder='Search' ng-model="searchText">
  <div ng-controller="itemsControllerNgInit" ng-init="init(<%= @lists %>)">
    <%= @lists.length %>
    <div ng-repeat="list in lists | filter:searchText">
      <a ng-href="http://www.wikipedia.com/wiki/{{list.title | titleFormatted}}" target="_blank">{{list.title}}</a>
    </div>
  </div>
</div>